<h1>ユーザー一覧</h1>

<table>
  <thead>
    <tr>
      <th>メールアドレス</th>
      <th>郵便番号</th>
      <th>住所</th>
      <th>自己紹介文</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.zip_code %></td>
        <td><%= user.address %></td>
        <td><%= user.profile %></td>
        <td><%= link_to '詳細', user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
